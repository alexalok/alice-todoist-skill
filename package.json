{
  "name": "alice-todoist-skill",
  "version": "0.1.0",
  "description": "Cloudflare Worker backend for a Yandex Alice skill integrating with Todoist",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "build": "wrangler deploy --dry-run --outdir dist --env ci",
    "deploy": "wrangler deploy",
    "dev": "wrangler dev --env local",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "yandex",
    "alice",
    "todoist",
    "cloudflare",
    "worker"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "@types/node": "^22.7.4",
    "prettier": "^3.2.5",
    "typescript": "^5.6.3",
    "vitest": "^4.0.6",
    "wrangler": "^4.45.3"
  },
  "dependencies": {
    "itty-router": "^4.2.1"
  }
}
